programa {
  funcao inicio() {
    inteiro chute, numeroMagico
    numeroMagico = 1
    escreva("Digite um número de 1 a 10: ")// pedir o chute do usuário
    leia(chute)// ler o chute do usuário
    // se chute for igual ao numeroMagico (chute == numeroMagico)
      // parabenizar o usuário por ter acertado
    // se não for igual
      // dizer que ele errou
    se(chute == numeroMagico){
      escreva("Parabéns, você acertou!")
    }senao{
      escreva("Eroooooooooooouuuu!")
    }

  }
}
